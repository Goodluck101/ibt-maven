pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        } //stage1 end
        stage('hi') {
            steps {
                echo 'Hello World'
            }
        } //stage2 end
        stage('hiya') {
            steps {
                echo 'Hello World, Whats up?'
            }
        } //stage3 end

    } //stages ending

} //Pipeline end