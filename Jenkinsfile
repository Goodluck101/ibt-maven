pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Name stage') {
            steps {
                echo 'this is uzoma'
            }
        }
        stage('inst stage') {
            steps {
                echo 'this is ibt institute'
            }
        }
    }
}//end of pipeline
