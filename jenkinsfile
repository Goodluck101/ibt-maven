pipeline {
    agent any

    stages {
        stage('Hello') {
            steps{
                echo "hello world"
            }
        } // stage1
        stage('Hi') {
            steps{
                echo "Hi this is fao"
            }
        }
        stage('Github download') {
            steps{
                git branch: 'jan2024_fao', credentialsId: 'fao_met_credentials', url: 'https://github.com/IBT-learning/ibt-maven.git'
            }
        }
    }
} // end of pipeline