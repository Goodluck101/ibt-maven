pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello IBT'
            }
            }
            stage('Hi') {
            steps {
                echo 'Hello Emmanuel'
            }
            }
            stage('Github download') {
            steps{
                git branch: 'jan2024_emmanuel', credentialsId: 'emmanuel_github_credentials', url: 'https://github.com/IBT-learning/ibt-maven.git'
            }
           } //stage3
           stage ('list Repo contents') {
           steps{
               sh 'ls -lrt'
               //step
            } //stage4
          } // end of stages