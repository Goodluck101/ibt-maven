pipeline {
    agent any

parameters{
  string( name:'branch_name', defaultvalue: 'main', description: 'enter branch to build')
}
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }//end of stage
         stage('hi') {
            steps {
                echo 'hi this is komivi'
             }
}

stage('Github download'){
    steps{
        git branch: '$branch_name', credentialsId: 'komivi_github_credential', url: 'https://github.com/IBT-learning/ibt-maven.git'
    }
  } //stages3
stage('list repo contents') {
   steps{
        bat 'dir'
    } //step
   } //stage4
 }//end of stages
}//end of pipeline
