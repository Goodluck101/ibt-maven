pipeline{
    agent any

    stages{
        stage('hello'){
            steps{
                echo "hello"
            }
        }
        stage("hii"){
            steps{
                echo "this is taiwo"
            }
        }
        stage("download from git"){
            steps{
                git branch: 'feature-taiwo3', credentialsId: 'teewana', url: 'https://github.com/IBT-learning/ibt-maven.git'
            }
        }
        stage("list files"){
            steps{
                bat 'dir'
            }
        }
    }
}