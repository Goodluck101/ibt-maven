pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }//end of stage
         stage('hi') {
            steps {
                echo 'hi this is komivi'
             }
}

stage('Github download'){
    steps{
        git branch: 'jan2024_komivi', credentialsId: 'komivi_github_credential', url: 'https://github.com/IBT-learning/ibt-maven.git'
    }
}
 }//end of stages
}//end of pipeline