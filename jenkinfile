pipeline{
  agent any
  stages{
     stage('hello world'){
       steps{
        echo "hello world, my name is oladimeji Akisanya"
       }
     } //stage 1
     stage('hi'){
         steps{
          echo "love my world"
         }
     } // stages 2
     stage('download fro GIT'){
       steps{
             git branch: 'demo1', changelog: false, credentialsId: 'GitHub_cred_oakisanyao', poll: false, url: 'https://github.com/akisanyao/git.git'
       }
     } //stage 3
     stage{
      steps(lists files) {
       bat'dir'
      }
     } //stage4

  } //stages
} //pipeline