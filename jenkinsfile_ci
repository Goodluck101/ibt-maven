pipeline
       {
     agent any

      stages{
           stage('Github checkout'){
                steps{
                     git branch: 'jan2024_emmanuel', credentialsId: 'emmanuel_github_credentials', url: 'https://github.com/IBT-learning/ibt-maven.git'

                }
           }
           stage('list contents'){
                steps{
                     sh 'ls -lrt'
                }
           }

           } // end of stages
} // end of pipeline