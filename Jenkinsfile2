    agent any

        stages {

        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hi') {
            steps {
                echo 'Hi this is Sagar practicing'
            }
        }
        stage('Download from Git') {
            steps {
               git branch: 'main', changelog: false, credentialsId: 'Github_cred_sagar', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'

                   }
                }
        stage('List Files') {
                    steps {
                       bat 'dir'
                        }                           }
                     }



        }

