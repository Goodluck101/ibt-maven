pipeline {
    agent any

    stages{

      stage('Github Checkout'){
        steps{
           git branch: 'feature-uzoma', changelog: false, credentialsId: 'github_creds_uzoma', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'


        }

      }

       stage('List Content) {
         steps{
            sh 'ls -lrt'
            }

       }


   } // end of stages

} // end of pipeline