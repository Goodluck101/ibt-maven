pipeline {
    agent any

    stages {
        stage('Github checkout'){
            steps{
                git branch: 'jan2024_gunjan', changelog: false, credentialsId: 'ibt', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'
            }
        }
        stage('List contents'){
            steps{
                sh 'ls -lrt'
            }
        }
    } // end of stages
} // end of pipeline