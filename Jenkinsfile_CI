pipeline{
    agent any

    stages{
        stage('Git checkout'){
            steps{
                git branch: 'josephine2', changelog: false, credentialsId: 'ibt', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'
               }
            }
        stages('validate'){
            steps{
                withMaven(maven: 'maven_3.9',) {
                    sh 'mvn validate'
                }
            }

        }

    }
}