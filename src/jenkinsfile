pipeline {
    agent any

    parameters{
        string(name:"Branch_Name", defaultValue: "main", description: "Enter branch to build")
    }

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        } // stage one
        stage('Hi') {
            steps {
                echo 'Hello this is shamrin'
            }
        }//stage two
        stage('Download from Git'){
            steps{
                echo 'Git Downloading'
            }
        }
    } //stages
} //pipeline
