  agent any

    tools{
        maven 'maven_3.8'
    }

  stages{
      stage('git checkout'){
          steps{
               git branch: 'feature_nasir1', changelog: false, credentialsId: 'GitHub_cred_nasir', poll: false, url: 'https://github.com/IBT-learning/ibt-maven.git'
         }
     }