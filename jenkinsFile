pipeline {
    agent any

    stages {
        stage('hello') {
            steps {
                echo "hello world"
            }
        }
        stage('hi') {
            steps {
                echo 'hi this is fao'
            }
        }
        stage('Github download') {
            steps{
                git branch: '$Branch_name', credentialsId: 'fao_met', url: 'https://github.com/IBT-learning/ibt-maven.git'
            }
        }
        stage('List repo contents') {
            steps{
                bat 'dir'
            }
        }
    }
}
