pipeline {
    agent any

    stages {
        stage('hello') {
            steps {
                echo "hello world"
            }
        }
        stage('hi') {
            steps {
                echo 'hi this is fao'
            }
        }
        stage('Github download') {
            steps{
                git branch: 'jan2024_fao', credentialsId: 'fao_met', url: 'https://github.com/IBT-learning/ibt-maven.git'
            }
        }
    }
}
